import java.util.ArrayList;
import java.util.Collections;


public class TestCompare {

	public static void main(String[] args) {
		int a1 = 10;
		int a2 = 15;
		int a3 = 8;
		
		ArrayList arr = new ArrayList();
		
		arr.add(a1);
		arr.add(a2);
		arr.add(a3);
		
		Collections.sort(arr);
		
		for(Object i : arr)
		{
			System.out.println("number is "+i);
		}

	}

}
